<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>

<% @events.each do |event| %>

<div class="event">
  <div><%= event.name %></div>
  <div><%= event.date %></div>
  <div><%= event.location %></div>
  <div><%= event.description %></div>
  <div><%= event.created_at %></div>
</div>
<% end %>
